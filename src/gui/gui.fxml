            </Tab>
            <Tab text="Ticket">
               <content>
                 	<fx:include fx:id="ticketTab" source="TicketTab.fxml" />
               </content>
            </Tab>
            <Tab text="Carnet">
               <content>
               		<fx:include fx:id="carnetTab" source="carnetTab.fxml"/>
               </content>
            </Tab>
            <Tab text="Seat">
               <content>
               		<fx:include fx:id="seatTab" source="seatTab.fxml"/>
               </content>
            </Tab>
            <Tab text="EventType">
               <content>
               		<fx:include fx:id="eventTypeTab" source="eventTypeTab.fxml"/>
               </content>
            </Tab>
            
        </tabs>
      </TabPane>
   </center>
</BorderPane>
